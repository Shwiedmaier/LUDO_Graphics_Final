      "LuaScript": "bag = Global.getVar(\"bag\")\npieceBag = Global.getVar(\"pieceBag\")\nstartingPos = nil\n\nfunction onLoad(save_state)\n    print \"moar tests\"\n    if startingPos == nil then\n        local sPos = Global.getTable(\"sPos\")\n        if sPos[self.getGUID()] != nil then startingPos = sPos[self.getGUID()]\n        else startingPos = self.getPosition() end\n    end\nend\n\nfunction onCollisionEnter(collision_info)\n\n    local lastHeld = Global.getVar(\"lastHeld\")\n\n    if Global.getVar(\"board\") != collision_info.collision_object.getGUID() then\n        if not compareColors(self, collision_info.collision_object) then\n            if collision_info.collision_object.getGUID() == lastHeld.getGUID() then\n                self.setPosition(startingPos, false, true)\n            end\n        elseif collision_info.collision_object.getPosition()['y'] < self.getPosition()['y'] then\n\n            local sPos = Global.getTable(\"sPos\")\n            sPos[self.getGUID()] = startingPos\n            sPos[collision_info.collision_object.getGUID()] = collision_info.collision_object.getTable(\"startingPos\")\n            Global.setTable(\"sPos\", sPos)\n\n            getObjectFromGUID(pieceBag).putObject(self)\n            getObjectFromGUID(pieceBag).putObject(collision_info.collision_object)\n\n            params = {}\n            -- params.position = {collision_info.collision_object.getPosition()['x'], collision_info.collision_object.getPosition()['y'], collision_info.collision_object.getPosition()['z']}\n            params.position = collision_info.collision_object.getPosition()\n            wall = getObjectFromGUID(bag).takeObject(params)\n            wall.setColorTint(collision_info.collision_object.getColorTint())\n\n            walls = Global.getTable(\"walls\")\n            currentPlayer = Global.getVar(\"currentPlayer\")\n\n            if walls[currentPlayer] == nil then walls[currentPlayer] = {} end\n\n            table.insert(walls[currentPlayer], wall.getGUID())\n\n            Global.setTable(\"walls\", walls)\n        end\n    end\nend\n\nfunction setStartingPosition(s)\n    startingPos = s\nend\n\nfunction compareColors(o1, o2)\n    r1 = o1.getColorTint()['r'];\n    g1 = o1.getColorTint()['g'];\n    b1 = o1.getColorTint()['b'];\n\n    r2 = o2.getColorTint()['r'];\n    g2 = o2.getColorTint()['g'];\n    b2 = o2.getColorTint()['b'];\n\n    if r1 == r2 and g1 == g2 and b1 == b2 then return true else return false end\nend\n",

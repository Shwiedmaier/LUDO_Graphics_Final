  "LuaScript": "board = \"9f2659\"\nbag = \"2aa177\"\npieceBag = \"7aae0a\"\nplayers = {\"Blue\", \"Green\", \"Yellow\", \"Red\"}\nwalls = {\"Blue\", \"Green\", \"Yellow\", \"Red\"}\n\nstartingZones = {\"Blue\", \"Green\", \"Yellow\", \"Red\"}\nstartingZones[\"Blue\"] = \"88711a\"\nstartingZones[\"Green\"] = \"63c0ec\"\nstartingZones[\"Yellow\"] = \"6769bb\"\nstartingZones[\"Red\"] = \"e1e870\"\n\ndie = \"e18fc7\"\nroll = nil\n\nsPos = {}\n\nlastHeld = nil\n\nplayers[\"Red\"] = {\"d11444\", \"06e2ed\", \"9bd478\", \"dd392c\"}\nplayers[\"Green\"] = {\"59d3f0\", \"51d02b\", \"05a7b5\", \"8e7868\"}\nplayers[\"Blue\"] = {\"58b3d5\", \"21ba3b\", \"0c4967\", \"76d966\"}\nplayers[\"Yellow\"] = {\"4458db\", \"b8df3f\", \"a9658e\", \"a0f471\"}\n\ncurrentPlayer = nil\npreviousPlayer = nil\n\nwallPickupPosition = nil\n\nwinZone = \"f151fe\"\nobjectInWinZone = false\n\nfunction onLoad(save_state)\n    print \"test\"\n    sPos = {}\n    -- set current player to first from list of seated, since host always goes first\n    currentPlayer = getSeatedPlayers()[1]\nend\n\nfunction onObjectPickedUp(player_color, picked_up_object)\n    lastHeld = picked_up_object\n\n    if isPlayersPiece(player_color, picked_up_object) and currentPlayer == player_color then\n        print \"woohoo\"\n    elseif isPlayersWall(player_color, picked_up_object) and currentPlayer == player_color then\n        wallPickupPosition = picked_up_object.getPosition()\n    else picked_up_object.reload() end\nend\n\nfunction onObjectDropped(player_color, dropped_object)\n    if isPlayersWall(player_color, dropped_object) and getWallMoveDistance(dropped_object, wallPickupPosition) then\n        pieces = getObjectFromGUID(pieceBag).getObjects()\n        local count = 0\n\n        for i = 1, tableLength(pieces), 1 do\n            if isPlayersPieceFromGUID(player_color, pieces[i].guid) then\n                params = {}\n\n                if count == 0 then\n                    params.position = wallPickupPosition\n                elseif count == 1 then\n                    params.position = dropped_object.getPosition()\n                else\n                    break\n                end\n\n                params.guid = pieces[i].guid\n                o = getObjectFromGUID(pieceBag).takeObject(params)\n                -- o.setTable(\"startingPos\", sPos[o.getGUID()])\n                count = count + 1\n            end\n        end\n\n        dropped_object.destruct()\n    elseif isPlayersPiece(player_color, dropped_object) and objectInWinZone then dropped_object.destruct() end\nend\n\nfunction onPlayerTurnStart(player_color_start, player_color_previous)\n    -- set current player to this player\n    currentPlayer = player_color_start\n\n    -- set previous player to previous player ;)\n    previousPlayer = player_color_previous\n\n    local objectsInStartingZone = getObjectFromGUID(startingZones[\"Blue\"]).getObjects()\n\n    for i = 1, tableLength(objectsInStartingZone), 1 do\n        objectsInStartingZone[i].lock()\n    end\n\n    math.randomseed(os.clock())\n    local dieObject = getObjectFromGUID(die)\n    local dieVal = math.random(1, 6)\n\n    dieObject.setValue(dieVal)\n\n    if dieVal == 6 then\n        for i = 1, tableLength(objectsInStartingZone), 1 do\n            objectsInStartingZone[i].unlock()\n        end\n    end\nend\n\nfunction onObjectEnterScriptingZone(zone, enter_object)\n    if zone.getGUID() == winZone then\n        getObjectFromGUID(winZone).highlightOn({0, 1, 0})\n        objectInWinZone = true\n    end\nend\n\nfunction onObjectLeaveScriptingZone(zone, leave_object)\n    if zone.getGUID() == winZone then\n        getObjectFromGUID(winZone).highlightOff()\n        objectInWinZone = false\n    end\nend\n\nfunction isPlayersPiece(player_color, picked_up_object)\n    for i = 1, 4, 1 do\n        if players[player_color][i] == picked_up_object.getGUID() then\n            return true\n        end\n    end\n    return false\nend\n\nfunction isPlayersPieceFromGUID(player_color, object_GUID)\n    for i = 1, 4, 1 do\n        if players[player_color][i] == object_GUID then\n            return true\n        end\n    end\n    return false\nend\n\nfunction isPlayersWall(player_color, picked_up_object)\n\n    if walls[player_color] != nil then\n        print(tableLength(walls[player_color]))\n\n        for i = 1, tableLength(walls[player_color]), 1 do\n            if walls[player_color][i] == picked_up_object.getGUID() then\n                return true\n            end\n        end\n    end\n    return false\nend\n\nfunction getWallMoveDistance(wall_object, old_wall_position)\n    print(old_wall_position['x'])\n    print(wall_object.getPosition()['x'])\n    if math.abs(old_wall_position['x'] - wall_object.getPosition()['x']) > 0.6 then return true\n    elseif math.abs(old_wall_position['z'] - wall_object.getPosition()['z']) > 0.6 then return true\n    else return false end\nend\n\nfunction tableLength(T)\n  local count = 0\n  for _ in pairs(T) do count = count + 1 end\n  return count\nend\n",
